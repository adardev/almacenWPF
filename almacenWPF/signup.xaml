<Window x:Class="almacenWPF.signup"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:almacenWPF"
        mc:Ignorable="d"
        Title="signup" Height="450" Width="800">
    <Grid>
        <TextBox Name="nombre" HorizontalAlignment="Center" Margin="0,73,0,0" TextWrapping="Wrap" Text="Nombre" VerticalAlignment="Top" Width="120"/>
        <PasswordBox Name="password" HorizontalAlignment="Center" Margin="0,230,0,0" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="apellido" IsHitTestVisible="False" HorizontalAlignment="Center" Margin="0,114,0,0" TextWrapping="Wrap" Text="Apellido" VerticalAlignment="Top" Width="120" TextChanged="nombre_Copy_TextChanged"/>
        <TextBox x:Name="fechanac" HorizontalAlignment="Center" Margin="0,147,0,0" TextWrapping="Wrap" Text="yyyy-MM-dd" VerticalAlignment="Top" Width="120" TextChanged="nombre_Copy_TextChanged"/>
        <TextBox x:Name="tipo" HorizontalAlignment="Center" Margin="0,184,0,0" TextWrapping="Wrap" Text="Tipo" VerticalAlignment="Top" Width="120" TextChanged="nombre_Copy_TextChanged"/>
        <Button Click="signupbtn" Content="Registrar Usuario" HorizontalAlignment="Center" Margin="0,275,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>